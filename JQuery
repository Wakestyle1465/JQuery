1.console.log ('Let's get ready to party with JQuery!')
2. $('article img').addClass(image-center)
3. $('article p:last-child').remove()
4. $('h1').css('fontSize', Math.floor(Math.random()*100) + 0)
5.('ol').append('<li> Hi! <\li>')
6. ('ol').replaceWith('<p> So sorry about that mess up! </p>')
7.  $(".form-control").on('keyup blur change', function () {
      let red = $(".form-control").eq(0).val();
      let blue = $(".form-control").eq(1).val();
      let green = $(".form-control").eq(2).val();
      $("body").css("background-color",
          "rgb(" + red + "," + green + "," + blue + ")");
    });
8. $('img').on('click', function(e){
 $('e.target').remove()
}

