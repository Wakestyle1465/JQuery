JS
$('#submit').on('click',function(evt){
    evt.preventDefault()
    let rating = $('#rating').val()
    let movieName = $('#title').val()
    let movie = (`Movie: ${movieName} <br> Rating: ${rating}`)
    $('ul').append(`<li> ${movie}</li>`)
    $('#title').val('')
    $('#rating').val('-5')
})

$('#delete').on('click',function(evt){
    evt.preventDefault()
    $('ul li:last-child').remove()
})
